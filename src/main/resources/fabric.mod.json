{
  "schemaVersion": 1,
  "id": "paperdoll",
  "version": "${version}",

  "name": "Paper Doll",
  "description": "Bedrock Paper Doll ported to Minecraft Java",
  "authors": [
    "Francy-Chan"
  ],
  "contact": {
    "homepage": "https://github.com/Francy-chan/PaperDoll-1.17",
    "sources": "https://github.com/Francy-chan/PaperDoll-1.17",
    "issues": "https://github.com/Francy-chan/PaperDoll-1.17/issues"
  },

  "license": "LGPL-2.1",
  "icon": "assets/paperdoll/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "net.dreemurr.paperdoll.PaperDoll"
    ],
    "modmenu": [
      "net.dreemurr.paperdoll.config.ConfigModMenu"
    ]
  },
  "mixins": [
    "paperdoll.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  },
  "custom": {}
}
